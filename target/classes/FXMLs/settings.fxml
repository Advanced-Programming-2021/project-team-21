<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import java.net.URL?>
<AnchorPane prefHeight="200.0" prefWidth="300.0" styleClass="backgroundSettings" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.DuelMenu">
   <children>
      <Label layoutX="114.0" layoutY="14.0" styleClass="title-label" text="Settings">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button layoutX="90.0" layoutY="68.0" mnemonicParsing="false" prefWidth="120.0" styleClass="buttonEntrance" onMouseClicked="#pause" text="Pause">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="90.0" layoutY="110.0" mnemonicParsing="false" onMouseClicked="#setAudio" styleClass="buttonEntrance" prefWidth="120.0" text="Audio">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="90.0" layoutY="154.0" mnemonicParsing="false" prefWidth="120.0" styleClass="buttonEntrance" onMouseClicked="#surrender" text="Surrender" textFill="RED">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
   <stylesheets>
      <URL value="@../CSS/CSS.css" />
   </stylesheets>
</AnchorPane>
