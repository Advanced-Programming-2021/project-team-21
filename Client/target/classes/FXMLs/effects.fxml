<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.CreateCard"
            prefHeight="400.0" prefWidth="500.0">
    <ScrollPane hbarPolicy="NEVER" styleClass="scroll-bar" fitToWidth="true">
        <VBox alignment="TOP_CENTER" spacing="10" prefHeight="400"
              prefWidth="600" id="mainVBox" styleClass="effectsStage">
            <HBox spacing="100" alignment="CENTER">
                <ChoiceBox value="Groups" id="effectChoiceBox" styleClass="choice-box" minWidth="100"/>
            </HBox>

        </VBox>

    </ScrollPane>

    <stylesheets>
        <URL value="@../CSS/CSS.css"/>
    </stylesheets>

</AnchorPane>
